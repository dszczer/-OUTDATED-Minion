{
    "name": "dszczer/minion",
    "description": "Powerful wrapper extending Silex, merging simplicity of Silex and usability of Symfony 2",
    "authors": [
        {
            "name": "Damian SzczerbiÅ„ski",
            "email": "dszczer@gmail.com"
        }
    ],
    "keywords": [
        "silex",
        "silex fat",
        "minion",
        "minion framework",
        "symfony light",
        "symfony2 light",
        "symfony 2 light"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.6 <=~7.0",
        "silex/silex": "1.3.*",
        "twig/twig": "~2.0-dev",
        "symfony/twig-bridge": "3.*",
        "monolog/monolog": "1.*",
        "knplabs/console-service-provider": "~1.0",
        "symfony/console": "2.8.*",
        "propel/propel": "2.*@dev",
        "propel/propel-service-provider": "2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "Minion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minion\\Tests\\": "tests",
            "Minion\\Tests\\Command\\": "tests/src/Command",
            "Minion\\Tests\\Controller\\": "tests/src/Controller"
        }
    },
    "bin": [
        "bin/console"
    ]
}